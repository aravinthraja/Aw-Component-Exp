{
  "name": "aw-component-exp",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/aw-component-exp/aw-component-exp.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve --es5",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "^1.17.3",
    "@storybook/addon-knobs": "^6.0.16"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-controls": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/html": "^6.0.16",
    "@storybook/web-components": "^6.0.16",
    "@types/jest": "25.2.3",
    "@types/puppeteer": "2.0.1",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "lit-html": "^1.3.0",
    "puppeteer": "2.1.1",
    "react-is": "^16.13.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
